{"ast":null,"code":"import axios from 'axios';\nimport WineRecommendation from './components/WineRecommendation.vue';\nexport default {\n  name: 'App',\n  components: {\n    WineRecommendation\n  },\n  data() {\n    return {\n      foodInput: 'Italian',\n      // Default value, you can set it to an empty string if you prefer\n      wineRec: null\n    };\n  },\n  methods: {\n    async fetchWineRecommendation() {\n      try {\n        const response = await axios.get(`http://localhost:8080/getPairing?query=${this.foodInput}`);\n        console.log(response.data); // Log the response to the console\n        this.wineRec = response.data;\n      } catch (error) {\n        console.error('Error fetching wine recommendation:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","WineRecommendation","name","components","data","foodInput","wineRec","methods","fetchWineRecommendation","response","get","console","log","error"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"content-box\">\n    <h1>Wine Pairing App</h1>\n    <label for=\"foodInput\">Enter a food item or cuisine:</label>\n    <br>\n    <input v-model=\"foodInput\" id=\"foodInput\" type=\"text\">\n    <br>\n    <button v-on:click=\"fetchWineRecommendation\">Get Wine Pairings</button>\n    <WineRecommendation :wineRec=\"wineRec\" />\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport WineRecommendation from './components/WineRecommendation.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    WineRecommendation,\n  },\n  data() {\n    return {\n      foodInput: 'Italian', // Default value, you can set it to an empty string if you prefer\n      wineRec: null,\n    };\n  },\n  methods: {\n    async fetchWineRecommendation() {\n      try {\n        const response = await axios.get(\n          `http://localhost:8080/getPairing?query=${this.foodInput}`\n        );\n        console.log(response.data); // Log the response to the console\n        this.wineRec = response.data;\n      } catch (error) {\n        console.error('Error fetching wine recommendation:', error);\n      }\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"mappings":"AAcA,OAAAA,KAAA;AACA,OAAAC,kBAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,wBAAA;MACA;QACA,MAAAC,QAAA,SAAAT,KAAA,CAAAU,GAAA,CACA,+CAAAL,SAAA,EACA;QACAM,OAAA,CAAAC,GAAA,CAAAH,QAAA,CAAAL,IAAA;QACA,KAAAE,OAAA,GAAAG,QAAA,CAAAL,IAAA;MACA,SAAAS,KAAA;QACAF,OAAA,CAAAE,KAAA,wCAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}